pragma solidity >= 0.5.0 < 0.9.0;

contract greatestAmongst3 {
    function comparison(int x, int y, int z) public pure returns(int) {
        if( x > y && x > z ) {
            return x;
        }

        else if( y > z ) {
            return y;
        }

        else{
            return z;
        }
    }
}
